list = [[x for x in line.split()] for line in open('C:/users/rwillia8/Desktop/Day 2.txt')]
#print(list)
valid = 0

for index, i in enumerate(list):
    low = int(i[0].split('-')[0])
    high = int(i[0].split('-')[1])
    letter = i[1].replace(':','')
    pswd = i[2]

    count = 0
    for i in pswd:
        if i == letter:
            count += 1

    if count >= low and count <= high:
        #print(low, high, letter, pswd)
        valid +=1

print(valid)
